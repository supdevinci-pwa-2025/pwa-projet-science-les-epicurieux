{
  "name": "Les épicurieux",
  "short_name": "S&T",
  "start_url": "/index",
  "background_color": "#f77f00",
  "theme_color": "#f77f00",
  "orientation": "any",
  "description": "Les épicurieux vous aides à faire des choses",
  "categories": [
    "productivity",
    "health"
  ],
  "lang": "fr",
  "scope": "/",
  "id": "/index.html",
  "display": "standalone",
  "dir": "ltr",
  "display_override": [
    "standalone",
    "browser"
  ],
  "launch_handler": {
    "client_mode": "auto"
  },
  "icons": [
    {
      "src": "assets/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "assets/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "assets/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "assets/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ], 
  "file_handlers": [
    {
        "action": "/open.html",
        "accept": {
            "text/csv": [".csv"]
        }
    }
  ],
  "share_target": {
    "action": "/.netlify/functions/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "file",
          "accept": ["image/*", "text/csv"]
        }
      ]
    }
  },
  "shortcuts": [
    {
      "name": "Ajouter de la science",
      "short_name": "Ajout science",
      "description": "Accéder rapidement à l'ajout de science",
      "url": "/index.html",
      "icons": [
        {
          "src": "/assets/manifest-icon-192.maskable.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Voir mes randoms",
      "short_name": "Mes randoms",
      "description": "Accéder à mes randoms",
      "url": "/random.html",
      "icons": [
        {
          "src": "/assets/manifest-icon-192.maskable.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "assets/screenshot.png",
      "sizes": "1920x1080",
      "type": "image/png"
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "scope_extensions": []
}